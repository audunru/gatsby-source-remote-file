{
  "name": "gatsby-source-remote-file",
  "description": "Fetch a remote file and use it as a node in Gatsby",
  "version": "0.2.0",
  "main": "dist/gatsby-node.js",
  "types": "dist/gatsby-node.d.ts",
  "author": "Audun Rundberg <audun@rundberg.no>",
  "license": "MIT",
  "dependencies": {
    "gatsby": "^2.31.1",
    "gatsby-source-filesystem": "^2.10.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "git-tag-version": "^1.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.10",
    "lint-staged": "^10.5.3",
    "mkdirp": "^1.0.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "rimraf dist && mkdirp dist && rollup -c",
    "develop": "tsc --module CommonJS --outDir .",
    "prepublishOnly": "yarn build",
    "style": "eslint . --ext .js,.ts --ignore-path .gitignore",
    "test:watch": "jest --watch",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "validate": "yarn style && yarn type-check && yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/audunru/gatsby-source-remote-file.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "bugs": {
    "url": "https://github.com/audunru/gatsby-source-remote-file/issues"
  },
  "homepage": "https://github.com/audunru/gatsby-source-remote-file"
}
